{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/packages/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "words": [
    "bdca",
    "clippy",
    "direnv",
    "ededed",
    "fbca",
    "ffcc",
    "ffffc",
    "genai",
    "isort",
    "liblaf",
    "liblaf's",
    "mypy",
    "numpy",
    "pydocstyle",
    "pylint",
    "pyplot",
    "pyright",
    "pyrightconfig",
    "pyvista",
    "shfmt",
